// User function Template for C++

class Solution {
  public:
    void InsertAtBottom(stack<int>&st, int x){
        if(st.empty()){
            st.push(x);
            return;
        }
        int temp=st.top();
        st.pop();
        InsertAtBottom(st,x);
        st.push(temp);
    }
    stack<int> insertAtBottom(stack<int> st, int x) {
        InsertAtBottom(st,x);
        return st;
    }
};

//USED RECURSION
